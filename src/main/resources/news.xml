<?xml version="1.0" encoding="UTF-8"?>

<config>
	<var-def name="newses">
            <xpath expression="/html/body/table[2]/tbody/tr/td[2]/table/tbody/tr[2]/td[1]/table/tbody/tr/td/table/tbody">
                <html-to-xml>
                    <http method="POST" url="${url}">
                    	<http-param name="year">2015</http-param>
                    	<http-param name="month">01</http-param>
                    	<http-param name="day">01</http-param>
                    	<http-param name="year1">2016</http-param>
                    	<http-param name="month1">02</http-param>
                    	<http-param name="day1">15</http-param>
                    </http>
                </html-to-xml>
            </xpath>
    </var-def> 
    
    
